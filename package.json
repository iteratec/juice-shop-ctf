{
    "name": "juice-shop-ctf-cli",
    "version": "1.2.0",
    "description": "Command line client to generate INSERT statements for CTFd with the OWASP Juice Shop challenges",
    "author": "Bjoern Kimminich <bjoern.kimminich@owasp.org> (https://www.owasp.org/index.php/User:Bjoern_Kimminich)",
    "contributors": [
        "Bjoern Kimminich",
        "Josh Grossman"
    ],
    "keywords": [
        "web security",
        "web application security",
        "webappsec",
        "owasp",
        "pentest",
        "pentesting",
        "security",
        "vulnerable",
        "vulnerability",
        "broken",
        "capture the flag",
        "ctf",
        "ctfd",
        "cli"
    ],
    "preferGlobal": true,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bkimminich/juice-shop-ctf.git"
    },
    "bugs": {
        "url": "https://github.com/bkimminich/juice-shop-ctf/issues"
    },
    "homepage": "https://www.owasp.org/index.php/OWASP_Juice_Shop_Project",
    "license": "MIT",
    "main": "index.js",
    "bin": {
        "juice-shop-fbctf": "bin/juice-shop-ctf.js"
    },
    "scripts": {
        "test": "standard && nyc --reporter=html --reporter=text mocha test/unit",
        "e2e": "nyc --reporter=text mocha test/e2e --slow=10000",
        "coverage": "nyc report --reporter=text-lcov | coveralls",
        "stryker": "stryker run stryker.conf.js"
    },
    "engines": {
        "node": "~4 ~6 ~7 ~8"
    },
    "dependencies": {
        "bluebird": "^3.4.7",
        "colors": "^1.1.2",
        "entities": "^1.1.1",
        "inquirer": "^3.0.1",
        "jssha": "^2.2.0",
        "path": "^0.12.7",
        "request": "^2.79.0",
        "request-promise": "^4.1.1",
        "striptags": "^3.1.0"
    },
    "devDependencies": {
        "ava": "^0.19.1",
        "chai": "^3.5.0",
        "chai-as-promised": "^6.0.0",
        "chai-spies": "^0.7.1",
        "coveralls": "^2.11.16",
        "inquirer-test": "^1.0.5",
        "lockfile": "^1.0.3",
        "mocha": "^3.2.0",
        "nyc": "^10.1.2",
        "rewire": "^2.5.2",
        "standard": "~8.6",
        "stryker": "^0.6.1",
        "stryker-api": "^0.5.0",
        "stryker-html-reporter": "^0.4.1",
        "stryker-mocha-runner": "^0.3.1"
    },
    "standard": {
        "env": [
            "node",
            "mocha",
            "chai"
        ],
        "globals": [
            "it",
            "describe",
            "beforeEach"
        ]
    },
    "nyc": {
        "report-dir": "./reports/coverage"
    }
}
